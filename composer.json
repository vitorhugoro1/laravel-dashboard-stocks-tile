{
    "name": "vitorhugoro1/laravel-dashboard-stocks-tile",
    "description": ":package_description",
    "keywords": [
        "vitorhugoro1",
        "laravel-dashboard-stocks-tile"
    ],
    "homepage": "https://github.com/vitorhugoro1/laravel-dashboard-stocks-tile",
    "license": "MIT",
    "authors": [{
        "name": "Vitor Hugo Rodrigues Merencio",
        "email": "vitorhugo.ro10@gmail.com",
        "homepage": "https://vitorhugoro1.github.io",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.58",
        "scheb/yahoo-finance-api": "^4.0",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "livewire/livewire": "^2.5",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.20",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "VitorHugoRo\\StockTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VitorHugoRo\\StockTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/testbench package:test",
        "test-coverage": "vendor/bin/testbench package:test --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VitorHugoRo\\StockTile\\StockServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
